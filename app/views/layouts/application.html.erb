<!DOCTYPE html>
<html>
<head>
  <title>Belegungstool</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div id="webseite">
		<div id="menue">
			<%= render "shared/menue" %>
		</div>
		<div id="content">
			<%= yield %>
		</div>
		<div id="footer">
			<%= render "shared/footer" %>
		</div>
	</div>
</body>
</html>
